{
  "name": "@nezuchan/i18n",
  "version": "1.2.0",
  "description": "A i18n Language manager.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --fix --ext ts",
    "build": "rimraf dist && tsup src && gen-esm-wrapper dist/index.js dist/index.mjs",
    "prepare": "npm run build"
  },
  "author": "KagChi",
  "license": "GPL-3.0",
  "devDependencies": {
    "@hazmi35/eslint-config": "^8.7.1",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.24",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "gen-esm-wrapper": "^1.1.3",
    "rimraf": "^4.4.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "@hazmi35/eslint-config/typescript"
    ],
    "rules": {}
  },
  "dependencies": {
    "@discordjs/collection": "^1.3.0",
    "@sapphire/pieces": "^3.6.0",
    "@sapphire/result": "^2.6.0",
    "@skyra/i18next-backend": "^1.1.0",
    "i18next": "^21.10.0"
  }
}
